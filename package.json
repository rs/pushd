        "node-gcm": "^1.0.0",
        "mpns": "2.1.0",
        "async": "~0.2.10",
        "express": "~3.20.2",
        "body-parser": "~1.9.0",
        "hiredis": "~0.4.1",
        "redis": "~1.0.0",
        "netmask": "~1.0.4",
        "winston": "~0.7.3",
        "wns": "git+https://github.com/futurice/wns.git#28bfe39a3be8c64f3d72b7c7a5cac77c049b137a"
    },
    "devDependencies":
    {
        "mocha": "*",
        "should": "*"
